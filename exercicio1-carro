int main(){
    setlocale(LC_ALL,"");
    float velocidade,distancia,tempo;
    
    
    
     printf("digite a velocidade do carro");
     scanf("%f",&velocidade);
     
     printf("digite digite a distacia ser pecorrida em km: ");
     scanf("%f",&distancia);
     
     tempo= distancia / velocidade;
  
     printf("o tempo necessaro para percorrer %.2f km: %.2f horas \n",distancia, tempo);
 return 0;
 }
